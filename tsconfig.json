{
  "compilerOptions": {
    "incremental": true,
    "outDir": "./out",
    "allowJs": true,
    "checkJs": false,
    "target": "ES2021",
    "module": "commonjs",
    "strict": true,
    "strictNullChecks": true,
    "types": ["node"],
    "typeRoots": ["@types"],
    "noImplicitOverride": true,
		"noImplicitReturns": true,
    "sourceMap": true
  },
  "include": ["./src/**/*"]
}
